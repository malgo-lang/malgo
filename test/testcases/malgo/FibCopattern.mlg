#c-style-apply
module {..} = import "../../../runtime/malgo/Builtin.mlg"
module {..} = import "../../../runtime/malgo/Prelude.mlg"

type Stream(a) = { head: a, tail: Stream(a) }

def zipWith = {
  #(f, xs, ys).head -> f(xs.head, ys.head),
  #(f, xs, ys).tail -> zipWith(f, xs.tail, ys.tail)
}

def fib = {
  #.head -> 1L,
  #.tail.head -> 1L,
  #.tail.tail -> zipWith(addInt64, fib, fib.tail)
}

def main = {
  printString (toStringInt64 (fib.tail.tail.tail.tail.tail.head))
}
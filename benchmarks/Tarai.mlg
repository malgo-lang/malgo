module Tarai = {
  module {..} = import Builtin
  module {..} = import Prelude

  infix 4 (<=)
  def (<=) = { x y -> leInt32 x y }

  infixl 6 (+)
  def (+) = { x y -> addInt32 x y }

  infixl 6 (-)
  def (-) = { x y -> subInt32 x y }

  def tarai = { x y z ->
    if (x <= y)
      {y}
      {tarai
        (tarai (x - 1) y z)
        (tarai (y - 1) z x)
        (tarai (z - 1) x y)}
  }

  def main = {
    tarai 12 6 0 |> toStringInt32 |> putStrLn
  }
}
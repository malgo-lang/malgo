extern println(str:String):Unit
extern print(str:String):Unit
extern print_int(i:Int):Unit

def a:Int = 42

def fib(n:Int):Int = {
  if (n <= 1) {
    1
  } else {
    fib(n-1) + fib(n-2)
  }
}

def main():Unit = {
  println("Hello, world!");
  print("fib(10) = ");
  print_int(fib(10));
  println("");
}
(define (main:int)
  (println "Hello world")

  (map (range 1 100)
       (fn (i:int)
           (print-int (* i i))
           (print " ")))
  (println "")

  0)

{--
def main(): int do
  println "Hello world"

  map (1..100) do i:int ->
    print-int (i * i)
    print " "
  end

  println ""

  0
end
--}
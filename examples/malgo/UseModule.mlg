module UseModule = {
  module {..} = import Builtin;
  module Prelude = import Prelude;
  module {add_Int32, (<|)} = import Prelude;

  infixr 9 (.);
  (.) = { f g x -> f (g x) };

  succ = { x -> add_Int32 x 1 };

  main = {
    Prelude.putStrLn "Hello, world!";
    Prelude.print_Int32 <| (succ . succ) 1
  };
}

-- Expected: Hello, world!
-- Expected: 3

-- -*- tab-width: 2 -*-

type Argv = Array String;

id x = x;

-- recursive function
fib n =
  if n <= 1
  then 1
  else let a = fib (n - 1) in
       let b = fib (n - 2) in
       a + b;

-- entry point
main : Argv -> Answer;
main argv =
  let f = id id
  in println (intToString ((id fib) 30)) exit;

let extern print_int : Int -> {} = "print_int" in
let extern newline : () -> {} = "newline" in
let fun fib(n) : Int =
  if n <= 1
  then 1
  else fib(n - 1) + fib(n - 2) in
let fun fib_loop(n : Int, acc : Int) : {} =
  print_int(fib(acc)); newline(); if n <= acc then {} else fib_loop(n, acc + 1) in
fib_loop(30, 0)

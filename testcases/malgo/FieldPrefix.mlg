module FieldPrefix = {
  module {..} = import Builtin;
  module {..} = import Prelude;

  type Point2D = { x : Float, y : Float };

  print2D = { { Point2D.x = x, y = y } ->
    printString (floatToString x);
    printString ", ";
    printString (floatToString y)
  };
  
  type Point3D = { x : Float, y : Float, z : Float };

  zero2D : { Point2D };
  zero2D = { { x = 0, y = 0 } };

  main = {
    print2D !zero2D
  };
}

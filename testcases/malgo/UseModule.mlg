module UseModule = {
  module {..} = import Builtin;
  module Prelude = import Prelude;
  module {addInt32, (<|), (.)} = import Prelude;

  def succ = { x -> addInt32 x 1 };

  def main = {
    Prelude.putStrLn "Hello, world!";
    Prelude.printInt32 <| (succ . succ) 1
  };
}

-- Expected: Hello, world!
-- Expected: 3

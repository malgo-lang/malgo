module TestPatSynRecon = {
  module {..} = import Builtin;
  module {..} = import Prelude;

  infixl 6 (+#);
  def (+#) = {x y -> addInt64# x y};
  
  def sum =
    { Cons (Int64# x) xs ->
        sum xs |> {(Int64# s) -> Int64# (x +# s)}
    | Nil -> Int64# 0L#
    };
  
  def main = {
    sum (Cons (Int64# 1L#) (Cons (Int64# 2L#) Nil))
      |> {i -> printInt64 i}
  };
}

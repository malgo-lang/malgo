(module
   "test/testcases/malgo/EmptyArguments.mlg"
   ((import "runtime/malgo/Builtin.mlg" all)
      (import "runtime/malgo/Prelude.mlg" all)
      (def getMessage (fn ((clause () (seq (do (string "Hello, World!")))))))
      (def
         main
         (fn
            ((clause
                (_)
                (seq (let msg (apply0 getMessage)) (do (apply putStrLn msg)))))))))
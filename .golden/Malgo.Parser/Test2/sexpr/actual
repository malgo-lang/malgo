(module
   "test/testcases/malgo/Test2.mlg"
   ((data RGB () ((R ()) (G ()) (B ())))
      (def
         rtob
         (fn ((clause ((con R ())) (seq (do B))) (clause (a) (seq (do a))))))
      (foreign malgo_print_string (-> String# (tuple)))
      (def
         main
         (fn
            ((clause
                ()
                (seq
                   (do
                      (apply
                         (fn
                            ((clause
                                ((con B ()))
                                (seq
                                   (do (apply malgo_print_string (string "OK")))))
                               (clause
                                  (_)
                                  (seq
                                     (do
                                        (apply
                                           malgo_print_string
                                           (string "WRONG")))))))
                         (seq (do (apply rtob R))))))))))))
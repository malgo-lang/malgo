(module
   "test/testcases/malgo/Bool.mlg"
   ((import "runtime/malgo/Builtin.mlg" all)
      (import "runtime/malgo/Prelude.mlg" all)
      (def
         if
         (codata
            ((apply (project (apply (project (apply # False) "then") _) "else") e)
               e)
            ((apply (project (apply (project (apply # True) "then") t) "else") _)
               t)))
      (def
         main
         (fn
            ((clause
                ()
                (seq
                   (do
                      (apply
                         (project
                            (apply
                               (project (apply if True) "then")
                               (fn
                                  ((clause
                                      ()
                                      (seq (do (apply putStrLn (string "True"))))))))
                            "else")
                         (fn
                            ((clause
                                ()
                                (seq (do (apply putStrLn (string "False"))))))))))))))))
(module
   "test/testcases/malgo/HelloImport.mlg"
   ((import "runtime/malgo/Builtin.mlg" all)
      (import "runtime/malgo/Prelude.mlg" all)
      (sig main (block (tuple)))
      (def
         main
         (fn
            ((clause
                ()
                (seq
                   (do (apply putStr (parens (apply String# (string "hello")))))
                   (do
                      (opapp
                         |>
                         putStrLn
                         (fn
                            ((clause
                                (f)
                                (seq
                                   (do
                                      (apply
                                         f
                                         (parens
                                            (apply String# (string " world"))))))))))))))))))
(module
   "test/testcases/malgo/EmptyConstructor.mlg"
   ((import "runtime/malgo/Builtin.mlg" all)
      (import "runtime/malgo/Prelude.mlg" all)
      (data Empty () ((Empty ())))
      (data Unit () ((Unit ((tuple)))))
      (def
         main
         (fn
            ((clause
                (_)
                (seq
                   (let x Empty)
                   (let y (apply Unit (tuple)))
                   (do (apply print x))
                   (do (apply print y)))))))))
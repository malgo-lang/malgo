(module
   "test/testcases/malgo/Test1.mlg"
   ((data Bool () ((True ()) (False ())))
      (def
         if
         (fn
            ((clause (True t _) (seq (do (apply t (tuple)))))
               (clause (False _ f) (seq (do (apply f (tuple))))))))
      (foreign malgo_print_string (-> String# (tuple)))
      (def
         main
         (fn
            ((clause
                (_)
                (seq
                   (do
                      (apply
                         (apply
                            (apply if True)
                            (fn
                               ((clause
                                   (_)
                                   (seq
                                      (do
                                         (apply
                                            malgo_print_string
                                            (string "True"))))))))
                         (fn
                            ((clause
                                (_)
                                (seq
                                   (do
                                      (apply malgo_print_string (string "False"))))))))))))))))
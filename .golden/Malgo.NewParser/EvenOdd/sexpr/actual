(module
   "test/testcases/malgo/EvenOdd.mlg"
   ((import "runtime/malgo/Builtin.mlg" all)
      (import "runtime/malgo/Prelude.mlg" all)
      (def
         even
         (fn
            ((clause ((boxed (int32 0))) (seq (do True)))
               (clause
                  (n)
                  (seq
                     (do
                        (apply odd (parens (apply (apply subInt32 (int32 1)) n)))))))))
      (def
         odd
         (fn
            ((clause ((boxed (int32 0))) (seq (do False)))
               (clause
                  (n)
                  (seq
                     (do
                        (apply even (parens (apply (apply subInt32 (int32 1)) n)))))))))
      (def
         main
         (fn
            ((clause
                ()
                (seq
                   (do
                      (apply
                         (fn
                            ((clause
                                (False)
                                (seq (do (apply putStrLn (string "False")))))
                               (clause
                                  (True)
                                  (seq (do (apply putStrLn (string "True")))))))
                         (parens (apply even (int32 10))))))))))))
(module
   "test/testcases/malgo/Test1.mlg"
   ((data Bool () ((True ()) (False ())))
      (def
         if
         (fn
            ((clause ((con True (t _))) (seq (do (apply t (tuple)))))
               (clause ((con False (_ f))) (seq (do (apply f (tuple))))))))
      (foreign malgo_print_string (-> String# (tuple)))
      (def
         main
         (fn
            ((clause
                ()
                (seq
                   (do
                      (apply
                         (apply
                            (apply
                               if
                               (fn
                                  ((clause
                                      ()
                                      (seq
                                         (do
                                            (apply
                                               malgo_print_string
                                               (string "False"))))))))
                            (fn
                               ((clause
                                   ()
                                   (seq
                                      (do
                                         (apply
                                            malgo_print_string
                                            (string "True"))))))))
                         True)))))))))